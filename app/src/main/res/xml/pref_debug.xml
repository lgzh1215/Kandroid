<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <SwitchPreference
        android:defaultValue="true"
        android:key="isDebugOn"
        android:summary="该功能未实现"
        android:title="开启调试"/>
    <SwitchPreference
        android:defaultValue="true"
        android:key="isSaveKcsApi"
        android:title="保存通信内容"/>
    <SwitchPreference
        android:defaultValue="true"
        android:dependency="isSaveKcsApi"
        android:key="isSaveKcsRequest"
        android:title="保存Request内容"/>
    <SwitchPreference
        android:defaultValue="true"
        android:dependency="isSaveKcsApi"
        android:key="isSaveKcsResponse"
        android:title="保存Response内容"/>

</PreferenceScreen>